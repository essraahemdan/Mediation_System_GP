<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.iti.mediationconfig.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddServer</servlet-name>
        <servlet-class>com.iti.mediationconfig.AddServer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>com.iti.mediationconfig.Register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteServer</servlet-name>
        <servlet-class>com.iti.mediationconfig.DeleteServer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateServer</servlet-name>
        <servlet-class>com.iti.mediationconfig.updateServer</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteServer</servlet-name>
        <url-pattern>/DeleteServer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateServer</servlet-name>
        <url-pattern>/updateServer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddServer</servlet-name>
        <url-pattern>/AddServer</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <listener>
        <description>HttpSessionListener</description>
        <listener-class>database.DatabaseConnector</listener-class>
    </listener>
</web-app>
